model Inquilino {
  id    Int     @id @default(autoincrement())
  nome  String
  email String  @unique
  telefone String
  idade Int
  cidade String
  TipoMoradia String
  seTrabalha String
  seEstuda String
  quantidadePessoas Int
  senha String
  @@map("inquilinos")
}

model Locador {
  id    Int     @id @default(autoincrement())
  nome  String
  email String  @unique
  telefone Int
  cidade String
  bairro String
  cep Int
  rua String
  caractImovel String
  defDePessoas String
  valorImovel Int
  senha String

  @@map("locadores")
}


model Anuncios {
  id    Int     @id @default(autoincrement())
  nome String
  telefone Int
  cidade String
  bairro String
  cep Int
  rua String
  caractImovel String
  defDePessoas String
  valorImovel Int
  senha String
  @@map("anuncios")
}