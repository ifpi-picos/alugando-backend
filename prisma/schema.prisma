// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String
  @@map("usuarios")
}
model Inquilino {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String
  @@map("inquilinos")
}
model Locador {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String
  @@map("locadores")
}
model Anuncios {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String
  @@map("anuncios")
}